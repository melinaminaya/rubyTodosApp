<%= stylesheet_link_tag("post") %>
<p style="color: green"> <%= notice %></p>
<div id="posts" class="text-center">
    <% @posts.each do |post| %>
        <% if post.user.present? %>
            <% if post.user.id == current_user.id %>
                <%= render "posts/mypost" %>
            <% end %>
        <%end%>
    <% end %>
</div>